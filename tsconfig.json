{
    "compilerOptions": {
        "experimentalDecorators": true,
        "outDir": "./dist/",
        "noImplicitAny": true,
        "sourceMap": true,
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "strict": false,
        "allowSyntheticDefaultImports": true,
        "watch": true,
        "baseUrl":".",
        "paths": {
            "@pages/*": ["./src/pages/*"],
            "@stores/*": ["./src/stores/*"],
            "@components/*": ["./src/components/*"],
            "@router/*": ["./src/router/*"],
            "@images/*": ["./src/images/*"],
            "@apis/*": ["./src/apis/*"],
            "@models/*": ["./src/models/*"],
            "@utils/*": ["./src/utils/*"],
        }
    },
    "exclude": ["dist", "node_modules"],
    "include": [
        "./src/**/*"
    ]
}